{
  "printWidth": 120,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "plugins": ["prettier-plugin-organize-imports", "@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^@", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
